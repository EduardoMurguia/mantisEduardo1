{
  "type": "object",
  "comment": "issueslist",
  "properties": {
    "viewIssuesLink": {
      "type": "action",
      "selector": ".subtle",
      "control": "action.link"
    },
    "ismyview": {
      "type": "identifier",
      "selector": ".subtle[href]",
      "control": "exists",
      "selectContains": "Assigned to Me (Unresolved)"
    }
  },
  "connect": [
    {
      "match": [
        "isViewissue",
        "isViewissue",
        "isMyView"
      ],
      "action": "viewIssuesLink"
    }
  ]
}
