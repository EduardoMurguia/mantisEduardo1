{
  "type": "object",
  "comment": "issueslist",
  "properties": {
    "isMyView": {
      "type": "identifier",
      "selector": ".subtle[href]",
      "control": "exists",
      "selectContains": "Assigned to Me (Unresolved)"
    },
    "viewIssuesLink": {
      "type": "action",
      "selector": ".subtle",
      "control": "action.link"
    },
    "issuelist": {
      "type": "array",
      "selectorAll": ".subtle",
      "items": {
        "type": "object",
        "properties": {
          "ID": {
            "type": "string",
            "selector": ".subtle",
            "control": "action.link"
          },
          "Severity": {
            "type": "string",
            "selector": ".subtle",
            "control": "action.link"
          },
          "Summary": {
            "type": "string",
            "selector": ".subtle",
            "control": "action.link"
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isViewissue",
        "isViewissue"
      ],
      "action": "viewIssuesLink"
    }
  ]
}
